<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.StackPane?>
<?import com.cctintl.jfx.controls.JFXButton?>
<?import com.cctintl.jfx.controls.JFXProgressBar?>
<?import javafx.scene.layout.VBox?>
<?import javafx.geometry.Insets?>

<StackPane fx:id="root" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
	<VBox spacing="50" alignment="BASELINE_CENTER" maxWidth="550" maxHeight="250" >
		<JFXProgressBar fx:id="progress1" progress="100"></JFXProgressBar>
		<JFXProgressBar></JFXProgressBar>

		<JFXProgressBar fx:id="progress2" styleClass="custom-c3d-progress-bar" progress="100"></JFXProgressBar>
		<JFXProgressBar styleClass="custom-c3d-progress-bar"></JFXProgressBar>

		<JFXProgressBar styleClass="custom-c3d-progress-bar-stroke"></JFXProgressBar>
		<StackPane.margin>	
			<Insets>
				<left>50</left>
				<top>10</top>
			</Insets>			
		</StackPane.margin>
	</VBox>
</StackPane>
	
